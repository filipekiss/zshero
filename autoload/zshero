#!/usr/bin/env zsh


__zshero::core::core::variables
__zshero::core::core::setup_colors

local arg="$1"

case "$arg" in
    install|info|adopt|add)
        shift
        __zshero::core::run_command \
            "$arg" \
            "$argv[@]"
        ;;
    "")
        __zshero::core::run_command "info"
        ;;

    *)
        __zshero::core::run_command \
            "debug"
        ;;
esac

return $status
